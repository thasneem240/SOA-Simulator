<Window x:Class="ClientGuiApp.ShowAllServicesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientGuiApp"
        mc:Ignorable="d"
        Title="ShowAllServicesWindow" Height="450" Width="800">
    <Grid>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="303,118,303,118" Name="listBox" SelectionChanged="listBox_SelectionChanged"/>
        <Label Content="" HorizontalAlignment="Center" Margin="0,337,0,0" VerticalAlignment="Top" Name ="status"/>
        <Button Content="Load All Services" HorizontalAlignment="Center" Margin="0,71,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Back" HorizontalAlignment="Center" Margin="0,368,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
